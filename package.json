{
  "name": "vue-filter-date-parse",
  "description": "Simple date parsing filter for Vue.js",
  "keywords": [
    "vue",
    "vue-filter"
  ],
  "license": "MIT",
  "version": "1.1.3",
  "author": "Eduard Nikolenko <info@eduardnikolenko.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduardnikolenko/vue-filter-date-parse.git"
  },
  "files": [
    "dist/vue-filter-date-parse.cjs.d.ts",
    "dist/vue-filter-date-parse.cjs.js",
    "dist/vue-filter-date-parse.esm.d.ts",
    "dist/vue-filter-date-parse.esm.js"
  ],
  "main": "dist/vue-filter-date-parse.cjs.js",
  "module": "dist/vue-filter-date-parse.esm.js",
  "types": "dist/vue-filter-date-parse.esm.d.ts",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:cjs": "rollup --config rollup.config.js --format cjs --file dist/vue-filter-date-parse.cjs.js",
    "build:esm": "rollup --config rollup.config.js --format esm --file dist/vue-filter-date-parse.esm.js",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.0.1",
    "@wessberg/rollup-plugin-ts": "^1.2.13",
    "rollup": "^1.29.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5",
    "vue": "^2.6.11"
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  }
}
